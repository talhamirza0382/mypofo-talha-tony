<!--Breadcrumb-->
<div class="container">
    <nav aria-label="breadcrumb" style="margin-top: 30px";>
        <ol class="breadcrumb bg-transparent " >
            <li class="breadcrumb-item bc-item text-info"><a href="/">Home</a></li>
            <li class="breadcrumb-item" aria-current="page">Blog</li>
        </ol>
    </nav>
</div>
<!-- Blog Page -->

<div class="container " style="margin-top: 40px;">

    <div class="jumbotron p-3 p-md-5 text-white rounded bg-dark">
        <div class="col-md-12 px-0">

            <h1 class="display-4 font-italic">BLOG !!</h1>
            <p class="lead my-3">Below are the frameworks and compilers we used to
                build this project .</p>
        </div>
    </div>

    <!-- blog -->
    {{#each blogs}}
    <div class="row">
        <div class="col-md-7">
            <a href="/blogs/{{_id}}">
                <img class="img-fluid rounded mb-3 mb-md-0" src="{{this.imagetwo}}" alt="fff">
            </a>
        </div>
        <div class="col-md-5">
            <h3>{{this.nametwo}}</h3>
            <p>{{this.paratwo}}</p>
        </div>
    </div>
    
    <hr>
    {{/each}}
</div>